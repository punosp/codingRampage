
<div id="page-wrapper">
    <div class="container-fluid">
        <div class="row">
            <div id="admin">
                <div class="col-lg-12">
                  {{#if message}}
                      <div class="alert alert-success" role="alert">
                        {{#each message}}

                            <p>{{this}}</p>

                        {{/each}}
                      </div>
                  {{/if}}

                  {{#if errors}}
                      <div class="alert alert-danger" role="alert">
                        {{#each errors}}

                            <p>{{this}}</p>

                        {{/each}}
                      </div>
                  {{/if}}
                    <div class="row">
                        <div class="col-sm-12">

                            <h3><b>Participants</b></h3>
                        </div>


                        <br>
                        <br>
                    </div>
                    <div class="row">
                        <div class="col-lg-12">
                            <div class=" table-responsive">
                                <table class='table table-striped table-bordered' id="table-demo">
                                    <thead>
                                    <tr>

                                        <th>Name</th>
                                        <th>Email</th>
                                        <th>Phone</th>
                                        <th></th>


                                    </tr>
                                    </thead>
                                    <tbody>
                                  {{#each users}}

                                      <tr data-id="{{ this.id }}" data-model="user">

                                          <td>{{this.name }}</td>
                                          <td>{{this.email }}</td>
                                          <td>{{this.phone }}</td>
                                          <td><a href="/admin/user/{{this.id}}/show" class="btn btn-sm btn-info"/>Show</td>

                                      </tr>
                                  {{/each}}
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
                <!-- /.col-lg-12 -->
            </div>
            <!-- /.row -->
        </div>
        <!-- /.container-fluid -->
    </div>


    <!-- /#page-wrapper -->




</div>
